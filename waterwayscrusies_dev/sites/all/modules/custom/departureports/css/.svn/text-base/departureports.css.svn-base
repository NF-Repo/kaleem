.contentbox-inner
{
    width:82% !important;
    margin:auto;
    padding-top:5%;
    padding-bottom:5%;
}

.departureTitle
{
    font-size: 38px;
    font-family: georgia;
    color: #00529b;
    line-height: 60px;
    font-weight: normal;
    padding-left: 0%;
    padding-top: 3%;
    text-transform: uppercase;
}
.portsList{
    margin-top:10px;
}
.bx-wrapper .bx-pager.bx-default-pager a{
    -moz-border-radius: 6px !important;
