        margin: 5% 0 !important;
    }
    .aboutusDownsliderInner .span8{
        width: 85% !Important;
        float: left !Important;
    }
    .aboutusDownsliderInner .left-line-margin{
        margin-left: 0 !important; 
    }
    .aboutusDownsliderInner .span2{  
        width:5% !Important;
    }
    #routeMaplinesholder .span5
    {
        width: 42%;
        float: left;
    }
    #routeMaplinesholder .span2
    {
        width: 14.8% !important;
        float: left;
    }
    #routeMaplinesholder .span5{
        width: 40.5% !important;
        float: left !important;
    }

}

@media (max-width: 979px) and (min-width: 768px){
    .aboutusDownsliderInner .span8{
        width:73% !Important;
    }
    .aboutusDownsliderInner .span2 {
        width:4% !important;
    }
    .aboutusDownsliderInner .left-line-margin{
        margin-left:8% !important;
    }
}
@media (min-width: 979px) and (max-width: 1160px) { 

    .featuredJobs{
        padding-left: 10px;
        padding-right: 10px;
    }

}

@media (min-width: 1160px) and (max-width: 1200px) {

    .featuredJobs{
        padding-left: 10px;
        padding-right: 10px;
    }
    .aboutusDownsliderInner{
        width: 450px !important;
        margin: 40px !important;
    }

}
