         float: left;
    }
    .giftcardHolder
    {
        width:100% !important;
        margin-left: 0px;
    }
    .redeemtext h3, .giftcardHolder h3, .redeemtext p, .giftcardHolder p, .purchase a, .voucherList h3, .voucherList span, .voucherList .date
    {
