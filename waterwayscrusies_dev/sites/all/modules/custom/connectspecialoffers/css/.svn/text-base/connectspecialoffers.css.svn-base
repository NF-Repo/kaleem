#content{
    display: block  !important;
    padding-bottom: 1%;
}


#menu
{
    position: static !important;
}
.booknow-content{
    height:46px;
    padding-top: 9%;
}
.connectMain {
width: 73.5% !important;
margin: 0px auto;
}

.connectTitle
{
    font-size: 46px;
    font-family:georgia;
    line-height: 50px;
    text-transform: uppercase;
    color: #454444;
    margin-bottom: 1.5%;
    margin-top: 8%;
}


/* submenu css start here */

.subMenu
{
    width:auto;
    position:absolute;
    border: #bc9b6a 1px solid;

}
.subMenu ul
{
    list-style:none;
    margin:0px;
}
.subMenu ul li
{
    float: left;
}
.subMenu ul li a
{
    background:rgba(252,251,249,0.8);
    color:#454444;
    font-family:georgia;
    font-size:15px;
    float:left;
    line-height:37px;
    text-decoration:none;
    padding-left:25px;
    padding-right:25px;
    text-shadow: none !important;
    text-transform: uppercase;
    cursor: pointer;
    transition: all ease-in-out 0.3s;
    -webkit-transition: all ease-in-out 0.3s;
    -moz-transition: all ease-in-out 0.3s;
    -o-transition: all ease-in-out 0.3s;
}

.subMenu ul li a:hover
{
    background: #454444 !important;
    color:#FFF;
    font-family:georgia;
    font-size:15px;
    float:left;
    line-height:37px;
    text-decoration:none;
    padding-left:25px;
    padding-right:25px;
    text-shadow: none !important;
}

.subMenu ul li a.active
{
    background: #454444 !important;
    color:#FFF;
    font-family:georgia;
    font-size:15px;
    float:left;
    line-height:37px;
    text-decoration:none;
    padding-left:25px;
    padding-right:25px;
    text-shadow: none !important;
}

.overviewMain
{
    margin-top: 10%;
    margin-bottom: 5%;
}
.region-content{
    margin-bottom:3%;
}
.galleryHolder{
    margin: 4% 0 0;
}

.socialWidgetTitle{
    font-size:16px;
    font-weight: bold;
}

.carousel-inner{
    margin-top: 12px;
}
.carousel-indicators{
    right: 40% !important;
    margin-top: 6px;
}

.specialoffers-list li{
width: 47%;
float: left;
margin-left: 4%;
}
.specialoffers-list li:first-child ,.specialoffers-list li:first-child .connectSpecialoffer{
    margin-left:0;
}
.specialoffers-list{
    list-style-type: none;
}
.specialoffers-list .sliderBoxInner{
    font-size: 16px;
    margin-top: 20%;
    width: 307px;
    margin-left: 26px;
    line-height: 25px;
}
.specialoffers-list .sliderBoxout,.specialoffers-list .sliderBoxover{
    height:235px !important;
}
.specialoffers-list .sliderBoxover{
    width: auto;
}
.specialoffers-list .sliderBox-first-inner{
    margin-left: 13%;
    margin-right: 11%;
    padding-top: 8%;
}
.specialoffers-list .title{
    text-transform: uppercase;
    font-size: 25px;
    margin: 2% 5%;
}
.specialoffers-list .sliderBoxInner a{
    font-size:16px;
}
.specialoffers-list .line{
    margin-top: 5%;
}
.redeemtext{

    width:60%;
    margin:auto;
}
.redeemtext h3, .giftcardHolder h3
{
    font-size: 30px;
    color:#454444;
    font-weight: normal;
}

.redeemtext p, .giftcardHolder p
{
    font-size: 16px;
    color:#818181;
    font-weight: normal;
}

.giftcardHolder h3{
    margin-bottom: 4%;
}
.specialofferImg img{
      width: 95%;
 height: 212px;
margin-top: 5%;
}
.voucherList{
    margin-left:20%;
    width: 80% !important;
}

.voucherList h3{
    font-size: 26px;
    color:#454444;
    line-height: 30px;
}

.voucherList span{
    font-size: 20px;
    color:#818181;
    font-weight: bold;
}
.connectSpecialofferInner .span2{
    margin-top: -4px;
}
.book-now .line{
margin-top: 25%;
}

.book-now{
   
    display:none;
}
.vocher-booknow{
    width:217px !important;
}
.purchase a{
    font-family: georgia;
    font-size: 15px;
    font-weight: 600;
    color: #00529b;
    text-decoration: none;
}
.purchase.row-fluid [class*="span"]{
    margin-left:1% !important;
}
.event-bgimage{
    height: 235px !important;
    width: 358px !important;
}


.connectSpecialoffer
{
    background: #fff;
    margin: 2%;
    border: #bc9b6a 10px solid;
    -moz-box-shadow: 0 0 10px 1px #888;
    -webkit-box-shadow: 0 0 10px 1px#888;
    box-shadow: 0 0 10px 1px #888;
    cursor: pointer;
}

.connectSpecialoffer:hover
{
    border: #fff 10px solid;
}

.connectSpecialofferInner
{
    margin: 10%;
    color: #00529b;
    text-align: center

}

.connectSpecialofferInner .offerTitle
{
    font-size: 26.34px;
    text-transform: uppercase;
    line-height: 35px;
}
.connectSpecialofferInner .offerDescription
{
    font-size: 16.34px;
    color:#818181;
}

.connectSpecialofferInner .offerRequest
{
    font-size: 16.34px;
    line-height: 30px;
    font-weight: bold;
}

@media (min-width: 979px) and (max-width: 1160px) { 
    
      .connectMain
    {
        width: 100%;
    }
    .voucherList{
        
        width: 99% !important;
    }
    .giftcardHolder
    {
        width: 90%;
        margin-left: 10%;
    }
}
@media (max-width: 979px){
    .redeemtext{
        width:100%;
    }
     .connectMain{
        width:90% !important;
    }
    .linepadding .line{
        margin-top:9%;
    }
    .voucherList{
        margin-left:0;
        width: 100% !important;
    }
    .voucherList .span4{
        width: 45% !important;
    }
    .book-now{
        margin-left:0;
    }
    .connectMain
    {
        width: 100%;
    }
    .giftcardHolder
    {
     margin: 5% auto !important;
    }

}
